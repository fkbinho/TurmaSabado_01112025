package poo_inicio;

public class Carro {

	//Atributos
	String marca;
	String modelo;
	int ano;
	
	//Construtor
	Carro(){
		
	}
	
	Carro(String marca){
		this.marca = marca;
	}
	
	//Métodos
	void ligar(){
		System.out.println("O carro " + modelo + " está ligado.");
	}
	
	void acelerar(){
		System.out.println("O carro " + modelo + " está acelerando.");
	}
}
